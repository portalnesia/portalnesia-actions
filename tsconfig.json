{
    "compilerOptions": {
      "allowJs": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "lib": ["es6"],
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "target": "es6",
      "baseUrl": ".",
      "outDir": "lib",
      "strictNullChecks": false
    },
    "exclude": ["node_modules","lib"],
    "include": ["**/*.ts"]
}